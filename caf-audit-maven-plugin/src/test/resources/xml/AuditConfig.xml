<AuditedApplication
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://cafaudit.github.io/audit-service/schema/AuditedApplication.xsd"
		xsi:schemaLocation="https://cafaudit.github.io/audit-service/schema/AuditedApplication.xsd"
>
	<ApplicationId>ProductX</ApplicationId>
	<AuditEvents>
		<AuditEvent>
			<TypeId>viewDocument</TypeId>
			<CategoryId>documentEvents</CategoryId>
			<Params>
				<Param>
					<Name>String_Param</Name>
					<Type>string</Type>
					<Description>Description for String_Param</Description>
				</Param>
				<Param>
					<Name>String_Fulltext_Param</Name>
					<Type>string</Type>
					<IndexingHint>fulltext</IndexingHint>
					<Description>Description for String_Fulltext_Param</Description>
				</Param>
				<Param>
					<Name>String_Keyword_Param</Name>
					<Type>string</Type>
					<IndexingHint>keyword</IndexingHint>
					<Description>Description for String_Keyword_Param</Description>
				</Param>
				<Param>
					<Name>String_With_Constraints_Param</Name>
					<Type>string</Type>
					<Constraints>
						<MinLength>1</MinLength>
						<MaxLength>32</MaxLength>
					</Constraints>
					<Description>Description for String_With_Constraints_Param</Description>
				</Param>
				<Param>
					<Name>Int16_Param</Name>
					<Type>short</Type>
					<Description>Description for Int16_Param</Description>
				</Param>
				<Param>
					<Name>Int32_Param</Name>
					<Type>int</Type>
					<Description>Description for Int32_Param</Description>
				</Param>
				<Param>
					<Name>Int64_Param</Name>
					<Type>long</Type>
					<Description>Description for Int64_Param</Description>
				</Param>
				<Param>
					<Name>Float_Param</Name>
					<Type>float</Type>
					<Description>Description for Float_Param</Description>
				</Param>
				<Param>
					<Name>Double_Param</Name>
					<Type>double</Type>
					<Description>Description for Double_Param</Description>
				</Param>
				<Param>
					<Name>Boolean_Param</Name>
					<Type>boolean</Type>
					<Description>Description for Boolean_Param</Description>
				</Param>
				<Param>
					<Name>Date_Param</Name>
					<Type>date</Type>
					<Description>Description for Date_Param</Description>
				</Param>
			</Params>
		</AuditEvent>
	</AuditEvents>
</AuditedApplication>
